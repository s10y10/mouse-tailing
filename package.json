{
  "name": "mouse-tailing",
  "private": false,
  "version": "0.2.0",
  "description": "屏幕鼠标拖尾效果",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/mouse-tailing.umd.cjs",
  "module": "./dist/mouse-tailing.js",
  "exports": {
    ".": {
      "import": "./dist/mouse-tailing.js",
      "require": "./dist/mouse-tailing.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s10y10/mouse-tailing.git"
  },
  "keywords": [
    "拖尾",
    "tail",
    "background",
    "effect"
  ],
  "author": "s10y10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s10y10/mouse-tailing/issues"
  },
  "homepage": "https://github.com/s10y10/mouse-tailing",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "publish": "npm publish --access=public --registry=https://registry.npmjs.org/",
    "publish:beta": "npm publish --access=public --registry=https://registry.npmjs.org/ --tag=beta"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vue-tsc": "^1.8.8"
  }
}